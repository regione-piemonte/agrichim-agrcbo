<ivysettings>
	<settings defaultCache="${basedir}/build/cache" defaultResolver="urlResolver"/>
	<resolvers>
		<url name="urlResolver">
			<artifact pattern=" http://<WEB_SERVER_HOST:PORT>/java/[organisation]/[module]/[revision]/lib/[artifact].[type]" />
			<artifact pattern=" http://<WEB_SERVER_HOST:PORT>/java /[organisation]/[module]/[revision]/conf/[artifact].[type]" />
			<ivy pattern=" http://<WEB_SERVER_HOST:PORT>/java/[organisation]/[module]/[revision]/ivy-[module].xml" />
		</url>
	</resolvers>
</ivysettings>